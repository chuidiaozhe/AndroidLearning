<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <Button
            android:id="@+id/btn_concat"
          style="@style/button_style"
            android:text="concat 操作符"
            />
        <Button
            android:id="@+id/btn_concatarray"
            style="@style/button_style"
            android:text="concatarray 操作符"
            />
        <Button
            android:id="@+id/btn_merge"
            style="@style/button_style"
            android:text="merge 操作符"
            />
        <Button
            android:id="@+id/btn_conncatarray_delayerror"
            style="@style/button_style"
            android:text="conncatArrayDelayError操作符"
            />
        <Button
            android:id="@+id/btn_mergearray_delayerror"
            style="@style/button_style"
            android:text="mergeArrayDelayError操作符"
            />
        <Button
            android:id="@+id/btn_zip"
            style="@style/button_style"
            android:text="zip操作符"
            />
        <Button
            android:id="@+id/btn_combinelatest"
            style="@style/button_style"
            android:text="combinelatest操作符"
            />
        <Button
        android:id="@+id/btn_combinelatestdelayerror"

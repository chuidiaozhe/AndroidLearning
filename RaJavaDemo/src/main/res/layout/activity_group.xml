        android:text="combinelatest操作符"
        />
        <Button
            android:id="@+id/btn_reduce"
            style="@style/button_style"
            android:text="Reduce操作符"
            />
        <Button
            android:id="@+id/btn_collect"
            style="@style/button_style"
            android:text="Collect操作符"
            />
        <Button
            android:id="@+id/btn_startWith"
            style="@style/button_style"
            android:text="startWith操作符"
            />
        <Button
            android:id="@+id/btn_startWithArray"
            style="@style/button_style"
            android:text="startWithArray操作符"
            />
        <Button
            android:id="@+id/btn_count"
            style="@style/button_style"
            android:text="count操作符"
            />
    </LinearLayout>

</ScrollView>